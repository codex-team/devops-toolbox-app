{
  "name": "devops-toolbox-app",
  "version": "0.0.9",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "start": "electron .",
    "dev": "electron ./src/index.ts",
    "bulid": "tsc",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "postversion": "git push && git push --tags"
  },
  "build": {
    "files": [
      "src/**",
      "public/**",
      "package.json",
      "node_modules"
    ],
    "win": {
      "target": "nsis"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codex-team/devops-toolbox-app.git"
  },
  "author": "CodeX",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/codex-team/devops-toolbox-app/issues"
  },
  "homepage": "https://github.com/codex-team/devops-toolbox-app#readme",
  "dependencies": {
    "@types/node": "^14.14.6",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.3.0",
    "electron-updater": "^4.3.5",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "eslint": "^7.13.0",
    "ts-node": "^9.0.0"
  }
}
